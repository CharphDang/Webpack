{
    "compilerOptions": {
        "module": "es6",
        "target": "es5",
        "sourceMap": true,
        "noEmit": false,
        "downlevelIteration": true,
        "checkJs": false,
        "allowJs": true,
        "lib": ["dom", "es5", "es2016"],
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": true
    },
    "include": ["./webapp/**/*.ts", "./webapp/typings/**/*.ts"],
    "exclude": ["./node_modules/**/*"]
}
